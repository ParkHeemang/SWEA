package test;

public class exam2 {

	static int[] arr; // arr[] : 데이터가 저장된 배열

	public static void main(String[] args) {

		// 재귀 호출을 통한 순열 생성

		// swap(i,j): arr[i] <--교환--> arr[j]
		int n = 4;
		int k = 0;// n: 원소의 갯수, k : 현재까지 선택된 원소의 수

		perm(n, k);

	}

	private static void perm(int n, int k) {
		if (k == n) {

			System.out.println(arr);
		}

	}

}
