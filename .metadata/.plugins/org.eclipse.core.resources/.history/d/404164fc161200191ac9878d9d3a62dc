package practice0104;

public class Bouncer {
	
	public void barkAnimal(Animal animal) {
		
		if(animal instanceof Tiger) {   //animal ¿Ã Tiger∞¥√º∂Û∏È
			System.out.println("æÓ»Ô");
			
		}else if (animal instanceof Lion) {
			System.out.println("¿∏∏£∑∑");
			
