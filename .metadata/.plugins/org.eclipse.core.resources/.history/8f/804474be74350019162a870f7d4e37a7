package problem1240;

import java.util.Scanner;

public class Solution {

	public static void main(String[] args) {

		Scanner sc = new Scanner(System.in);

		int T = sc.nextInt();

		for (int t = 1; t <= T; t++) {

			int[] code = new int[8];
			int code_count = 0;

			int N = sc.nextInt();
			int M = sc.nextInt();

			int[][] barcode = new int[N][M];

			for (int i = 0; i < barcode.length; i++) {

				String line = sc.next();

				for (int j = 0; j < barcode[i].length; j++) {

					barcode[i][j] = Character.getNumericValue(line.charAt(j));

				}

			}

			int start_N = -1;

			for (int i = 0; i < barcode.length; i++) {

				boolean find = false;

				for (int j = 0; j < barcode.length; j++) {

					if (barcode[i][j] != 0) {

						start_N = i;
						find = true;
						break;

					}

				}

				if (find) {
					break;

				}

			}

			System.out.println(start_N);

			for (int i = 0; 7 * i + 6 < barcode[start_N].length; i++) { // 열을 도는 반복문

				String number = "";

				for (int j = 0; j <= 6; j++) {

					number += barcode[start_N][7 * i + j];

				}

				if (number.equals("0000000")) {

					continue;

				} else if (number.equals("0001101")) {

					code[code_count++] = 0;

				} else if (number.equals("0011001")) {

					code[code_count++] = 1;

				} else if (number.equals("0010011")) {

					code[code_count++] = 2;

				} else if (number.equals("0111101")) {

					code[code_count++] = 3;

				} else if (number.equals("0100011")) {

					code[code_count++] = 4;

				} else if (number.equals("0110001")) {

					code[code_count++] = 5;

				} else if (number.equals("0101111")) {

					code[code_count++] = 6;

				} else if (number.equals("0111011")) {

					code[code_count++] = 7;

				} else if (number.equals("0110111")) {

					code[code_count++] = 8;

				} else if (number.equals("0001011")) {

					code[code_count++] = 9;

				}

			}
			
			
			
			for (int i = 0; i < code.length; i++) {
				
				
				System.out.print(code[i]);
				
				
				
			}
			
			System.out.println();
			
			
			
			
			
			
			
			

		}

	}
}
