
public class exam3 {

	public static void main(String[] args) {

		int[] bit = new int[4];

		for (int i1 = 0; i1 <= 1; i1++) {
			bit[0] = i1;
			for (int i2 = 0; i2 <= 1; i2++) {
				bit[1] = i2;
				for (int i3 = 0; i3 <= 1; i3++) {
					bit[2] = i3;
					for (int i4 = 0; i4 <= 1; i4++) {
						bit[3] = i4;
						System.out.println(i1+", " +i2+", "+ i3+", "+ i4);
						
						
					}

				}

			}

		}

	}

}
